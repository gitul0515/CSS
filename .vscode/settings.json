{
    "editor.fontSize": 20,
    "liveServer.settings.port": 5501,

    // Settings About SASS
    "liveSassCompile.settings.formats": [
    {
        "format": "expanded",
        "extensionName": ".css",
        "savePath": "~/css" // scss 파일이 있는 폴더의 하위 css 폴더
    }
    ], 
    "liveSassCompile.settings.excludeList": [ 
    "**/node_modules/**",
    ".vscode/**" 
    ],
    "liveSassCompile.settings.includeItems": [
    "path/subpath/a.scss",
    "path/subpath/b.scss",
    ],
    "liveSassCompile.settings.autoprefix": [
    "> 1%",
    "last 2 versions"
    ]
}